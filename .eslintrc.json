{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "no-alert": 0,
    "no-underscore-dangle": 0,
    "max-len": 0,
    "class-methods-use-this": 0
  }
}
